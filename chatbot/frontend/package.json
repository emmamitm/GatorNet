{
    "name": "chatbot-frontend",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:5001",
    "dependencies": {
        "@phosphor-icons/react": "^2.1.7",
        "@tailwindcss/cli": "^4.0.0",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^1.7.9",
        "leaflet": "^1.9.4",
        "leaflet-routing-machine": "^3.2.12",
        "motion": "^12.3.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-dropzone": "^14.3.8",
        "react-router": "^7.1.5",
        "react-scripts": "^5.0.1",
        "react-spinners": "^0.15.0",
        "tailwindcss": "4.0.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "concurrently": "^9.1.2"
    },
    "scripts": {
        "start": "concurrently \"npm run start:react\" \"npm run start:tailwind\"",
        "start:react": "react-scripts start",
        "start:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/css/output.css --watch",
        "build": "concurrently \"npm run build:react\" \"npm run build:tailwind\"",
        "build:react": "react-scripts build",
        "build:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/css/output.css --minify",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
